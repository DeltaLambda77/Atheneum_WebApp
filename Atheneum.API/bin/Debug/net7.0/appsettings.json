{
  "ConnectionStrings": {
    "AtheneumAppDbConnection": "Server=localhost\\sqlexpress;Database=AtheneumDb;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=False"
  },
  "SeriLog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "JwtSettings": {
    "Issuer": "AtheneumAPI",
    "Audience": "AtheneumAPIClient",
    "Duration": 1
  },
  "AllowedHosts": "*"
}
